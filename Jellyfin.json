            "type": "loki",
            "uid": "lokithedatasourceuid"
          },
          "direction": "backward",
          "editorMode": "code",
          "expr": "count_over_time({container=\"Familyfin\"} |~ \"FFmpeg exited with code|Deleting partial stream|Playback stopped|Stopping ffmpeg\" [$__auto])",
          "hide": false,
          "instant": false,
          "legendFormat": "Streams stopping",
          "queryType": "range",
          "range": true,
          "refId": "B"
